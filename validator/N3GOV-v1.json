{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "AlternativeSnapshotVotingSystems": {
            "enum": [
                "approval",
                "ranked-choice",
                "single-choice",
                "weighted"
            ],
            "type": "string"
        },
        "Kind": {
            "enum": [
                "custodial-treasury-management",
                "expense",
                "meta",
                "parameter-change",
                "proclamation",
                "treasury-management"
            ],
            "type": "string"
        }
    },
    "properties": {
        "agreement": {
            "properties": {
                "agreementId": {
                    "type": "number"
                },
                "agreementsFramework": {
                    "type": "string"
                },
                "chainId": {
                    "enum": [
                        1
                    ],
                    "type": "number"
                }
            },
            "required": [
                "agreementId",
                "agreementsFramework",
                "chainId"
            ],
            "type": "object"
        },
        "content": {
            "anyOf": [
                {
                    "properties": {
                        "amount": {
                            "type": "number"
                        },
                        "chainId": {
                            "enum": [
                                1
                            ],
                            "type": "number"
                        },
                        "recipient": {
                            "type": "string"
                        },
                        "token": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "amount",
                        "chainId",
                        "recipient",
                        "token"
                    ],
                    "type": "object"
                },
                {
                    "properties": {
                        "chainId": {
                            "enum": [
                                1
                            ],
                            "type": "number"
                        },
                        "erc20Approval": {
                            "properties": {
                                "amount": {
                                    "type": "number"
                                },
                                "chainId": {
                                    "enum": [
                                        1
                                    ],
                                    "type": "number"
                                },
                                "recipient": {
                                    "type": "string"
                                },
                                "token": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "amount",
                                "chainId",
                                "recipient",
                                "token"
                            ],
                            "type": "object"
                        },
                        "method": {
                            "type": "string"
                        },
                        "parameters": {
                            "items": {
                                "type": [
                                    "string",
                                    "number"
                                ]
                            },
                            "type": "array"
                        },
                        "target": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "chainId",
                        "method",
                        "parameters",
                        "target"
                    ],
                    "type": "object"
                },
                {
                    "properties": {
                        "prURI": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "prURI"
                    ],
                    "type": "object"
                },
                {
                    "properties": {
                        "choices": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        },
                        "statement": {
                            "type": "string"
                        },
                        "votingSystem": {
                            "$ref": "#/definitions/AlternativeSnapshotVotingSystems"
                        }
                    },
                    "required": [
                        "choices",
                        "statement",
                        "votingSystem"
                    ],
                    "type": "object"
                }
            ]
        },
        "critical": {
            "type": "boolean"
        },
        "discussion": {
            "type": "string"
        },
        "id": {
            "type": "number"
        },
        "kind": {
            "$ref": "#/definitions/Kind"
        },
        "spec": {
            "type": "number"
        },
        "votes": {
            "items": {
                "anyOf": [
                    {
                        "properties": {
                            "passed": {
                                "type": "boolean"
                            },
                            "uri": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "passed",
                            "uri"
                        ],
                        "type": "object"
                    },
                    {
                        "allOf": [
                            {
                                "properties": {
                                    "passed": {
                                        "type": "boolean"
                                    },
                                    "uri": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "passed",
                                    "uri"
                                ],
                                "type": "object"
                            },
                            {
                                "properties": {
                                    "outcome": {
                                        "anyOf": [
                                            {
                                                "items": {
                                                    "type": "string"
                                                },
                                                "type": "array"
                                            },
                                            {
                                                "type": "string"
                                            }
                                        ]
                                    }
                                },
                                "required": [
                                    "outcome"
                                ],
                                "type": "object"
                            }
                        ]
                    }
                ]
            },
            "type": "array"
        }
    },
    "required": [
        "content",
        "discussion",
        "id",
        "kind",
        "spec"
    ],
    "type": "object"
}

